

<div class="container">
  <div class="row center">
    <h1>List of All Dogs</h1>
  </div>

  <div class="row center">
      <%= will_paginate %>

          <ul class="dogs center">
            <% @dogs.each do |dog| %>
              <li><%= link_to dog.name, user_dog_path(@user, dog) %></li>
            <% end %>
          </ul>

      <%= will_paginate %>
  </div>

</div>