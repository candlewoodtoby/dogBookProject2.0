
<div class="container">
  <div class="row">
     <h1>Upload Your Dog's Photo</h1>
  </div>

  <div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= form_for(@photo, url: user_dog_photos_path) do |f| %>

            <%= f.file_field :cloud_id %>

          <%= f.submit "Upload Dog Photo", class: "btn btn-primary" %>
        <% end %>
    </div>
  </div>
</div>

